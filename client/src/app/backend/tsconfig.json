{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": ".",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "paths": {
            "*": [
                "../../node_modules/*",
                "../backend*"
            ]
        }
    },
    "include": [
        "./*.ts"
    ],
    "exclude": [
        "../../node_modules"
    ]
  }
  